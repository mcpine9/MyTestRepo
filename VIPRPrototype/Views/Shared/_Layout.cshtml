<html class="no-js">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    @Styles.Render("~/css")
    @RenderSection("PageCss", required: false)
    @Scripts.Render("~/bundles/modernizr")
    <!--[if gt IE 9]>
      <style type="text/css">
        .gradient 
        {
           filter: none;
        }
      </style>
    <![endif]-->
</head>
<body>
    <!--[if lt IE 7]>
            <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
        <![endif]-->
    <header id="LogIn">
        <span id="FacilityInformation">
            <h1>Moore Hospital</h1>
        </span>
        <span id="LogInNavigation">
            <a href="#user">Dr. Bunsen Honeydew</a> | <a href="#LogIn out">Log Out</a>
        </span>
    </header>
    <section id="MainSection">
        <div id="ContentWrapper">
            <nav id="LeftNav">
                <div id="LeftNavTabs">
                    <div id="btnPatientSearch"></div>
                    <div id="btnPatientChart"></div>
                    <div id="btnInbox"></div>
                </div>
            </nav>
            <section id="Content">
                @RenderBody()
            </section>
            <div class="clear"></div>
        </div>
    </section>
    <footer></footer>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("PageJs", required: false)
</body>
</html>
